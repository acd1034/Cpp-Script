{
  // https://code.visualstudio.com/docs/editor/userdefinedsnippets
  "main": {
    "prefix": "main",
    "body": [
      "#include <iostream>",
      "int main() {",
      "  $0",
      "}",
      ""
    ]
  },
  "struct": {
    "prefix": "struct",
    "body": [
      "struct $1 {",
      "  $0",
      "};",
      ""
    ]
  },
  "cout": {
    "prefix": "cout",
    "body": "std::cout << $0 << std::endl;"
  },
  "temp": {
    "prefix": "temp",
    "body": "template <typename T$0>"
  },
  "lambda": {
    "prefix": "lambda",
    "body": "[](${1:const auto&} x){ return $2; }"
  },
  "enable": {
    "prefix": "enable",
    "body": "enable_if_t<$0> = nullptr"
  },
  "senable": {
    "prefix": "senable",
    "body": "std::enable_if_t<$0, std::nullptr_t> = nullptr"
  },
  "adl": {
    "prefix": "ADL",
    "body": "using std::begin, std::end; // for ADL"
  },
}
